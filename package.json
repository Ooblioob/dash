{
  "name": "reacttesting",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "postinstall": "bower install",
    "test": "jest",
    "build-js": "browserify src/js/app.js -o src/bundle.js",
    "post-build": "onchange src/bundle.js -- npm run test-and-move",
    "test-and-move": "npm run move-app & npm test",
    "move-app": "cp -r src/* ../devdash/devdash/static && mv ../devdash/devdash/static/index.html ../devdash/devdash/static/dash.html",
    "watch": "watchify src/js/app.js -d -o src/bundle.js -v & npm run post-build"
  },
  "jest": {
    "scriptPreprocessor": "test_preprocessor.js",
    "rootDir": "src/js",
    "unmockedModulePathPatterns": [
      "node_modules/react"
    ],
    "testFileExtensions": [
      "coffee",
      "js"
    ]
  },
  "browserify": {
    "transform": [
      "reactify",
      "coffeeify",
      "envify"
    ]
  },
  "author": "",
  "license": "CC0",
  "devDependencies": {
    "browserify": "^8.1.3",
    "coffee-react": "~2.4.1",
    "coffeeify": "^1.0.0",
    "envify": "^3.2.0",
    "gulp-eslint": "^0.5.0",
    "gulp-less": "^3.0.1",
    "gulp-minify-css": "^0.4.6",
    "gulp-minify-html": "^1.0.0",
    "gulp-rev": "^3.0.1",
    "gulp-uglify": "^1.1.0",
    "gulp-usemin": "^0.3.11",
    "jest-cli": "~0.2.2",
    "onchange": "^0.2.0",
    "react-templates": "^0.1.13",
    "react-tools": "~0.12.2",
    "reactify": "^1.0.0",
    "uglify-js": "~2.4.16",
    "watchify": "^2.3.0"
  },
  "dependencies": {
    "coffee-react": "~2.4.1",
    "gulp": "~3.8.11",
    "gulp-concat": "~2.5.0",
    "gulp-if": "^1.2.5",
    "gulp-order": "^1.1.1",
    "gulp-react": "^2.0.0",
    "gulp-uglify": "^1.1.0",
    "jest-cli": "~0.2.2",
    "lazypipe": "^0.2.2",
    "merge-stream": "^0.1.7",
    "react": "~0.12.2",
    "react-tools": "~0.12.2",
    "yargs": "~3.0.4",
    "jquery": "^2.1.3",
    "keymirror": "~0.1.1",
    "object-assign": "^2.0.0",
    "flux": "^2.0.1"
  }
}
